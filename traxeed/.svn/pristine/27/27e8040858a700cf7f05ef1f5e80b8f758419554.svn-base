 */


class pagesmanager extends traxeed {
		
		public function __constract()
		{
			
		}

		public function getpagedata($pageid)
		
		{
			global $traxeed_mysqliDB;
			
			$query = "SELECT * FROM pagemanager WHERE id = ?";
			$params = array($pageid);
			$pagedata = $traxeed_mysqliDB->selectone($query,'s',$params);
			return $pagedata;
		}
	
}

$pagesmanager = new pagesmanager();
