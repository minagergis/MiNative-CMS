}
a:hover {
	outline: none;
	text-decoration: none;
}
a:focus {
	outline: none;
	outline-offset: 0;
	text-decoration: none;
}
a {
	-webkit-transition: 300ms;
	-moz-transition: 300ms;
	-o-transition: 300ms;
	transition: 300ms;
}
.tcenter {
	text-align: center;
	margin: auto;
}

.blocksheader {
}
.blocksheader h2 {
	color: #06608F;
	font-size: 15px;
	font-weight: 700;
	margin: 0 auto 12px;
	text-align: center;
	text-transform: uppercase;
	position: relative;
	z-index: 3;
	letter-spacing: 1px;
}
.blocksheader h2:after {
	content: " ";
	position: absolute;
	border: 1px solid rgba(0, 167, 228, 0.16);
	bottom: 8px;
	left: 0;
	width: 100%;
	height: 0;
	z-index: -2;
}
.blocksheader h2:before {
	content: " ";
	position: absolute;
	background: #fff;
	bottom: -6px;
	left: 50%;
	width: 130px;
	height: 30px;
	z-index: -1;
	margin-left: -65px;
}
.blocksheader h3 {
	color: #06608F;
	font-family: 'Droid Sans', sans-serif;
	font-size: 14px;
	font-weight: 600;
	margin: 0 auto 10px;
	text-align: center;
}
