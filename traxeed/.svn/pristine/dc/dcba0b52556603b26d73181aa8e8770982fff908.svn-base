<section>
	<div class="container">
		<div class="row">
			{include file="sidemenu.html"}

			<div class="col-sm-9 col-md-9 col-lg-9 float02">
				<div class="row">
					
					<div class="pagecontents">
						<div class="panel panel-default">
							<div class="panel-heading">
								{$pagetitle}
								<div class="float01">
								<button class="btn btn-default" onClick="window.print('.panel-body')">
									Print this page
								</button>
								<button class="btn btn-default" onClick="window.print()">
									Save as html
								</button>
								<button class="btn btn-default" onClick="window.print()">
									Save as pdf
								</button>
								</div>
							</div>
							<div class="panel-body">
								{if $status eq 0}
								<div class="col-md-12">
									<div id="output" class="alert alert-success message3">
										Ooops, This page is disabled!
									</div>
								</div>
								{else}
								With : {$pagewriter} | At : {$pagepubdate}
								{$pagecontent}
								{/if}
							</div>
							<div class="panel-footer">
								Tags : {$pagetags}
								{if $pageattachment != 0}
								<br>
								Attachments : <a href="contents/pages/{$pageattachment}">{$pageattachment}</a>
								{/if}
								</div>
						</div>

					</div>

				</div>
			</div>
		</div>

</section>

